<h1>Home#index</h1>
<br/>
<table class="table table-hover">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Show</th>
    </tr>
  </thead>
  <tbody>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.name %></td>
      <td><%= user.email %></td>
      <td><%=link_to user.name, user %></td>
    </tr>
  <% end %>
  </tbody>
</table>
<br/>
<p>Find me in app/views/home/index.html.erb</p>
